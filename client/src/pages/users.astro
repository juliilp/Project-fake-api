---
import Layout from "../layouts/Layout.astro";
import arrayUsers from '../helpers/arrayUsers'
import ComponentUser from "../components/componentUser.astro";
---

<Layout title="Usuarios" >
    <h1 class="font-bold text-[52px] font-sans" >Users</h1>
    {
        arrayUsers.map((user,key) => {
            return <ComponentUser
            key={key}
            titulo={user.titulo}
            descripcion={user.descripcion}
            request={user.request}
            response={JSON.stringify(user.response)}
            />
        })
    }
</Layout>