---
import Layout from "../layouts/Layout.astro";
import arrayMercadoPago from '../helpers/arrayMercadoPago'
import ComponenteObjeto from "../components/componenteObjeto.astro";
---

<Layout title="Usuarios" >
    <h1 class="font-bold text-[52px] font-sans" >Mercadopago</h1>
    {
        arrayMercadoPago.map((user,key) => {
            return <ComponenteObjeto
            key={key}
            titulo={user.titulo}
            descripcion={user.descripcion}
            request={user.request}
            response={JSON.stringify(user.response)}
            />
        })
    }
</Layout>